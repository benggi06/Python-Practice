   "source": [
    "n = int(input('N을 입력하세요.: '))\n",
    "x = int(input('X를 입력하세요.: '))\n",
    "\n",
    "a = list(input('숫자들을 입력하세요.: ').split(','))\n",
    "\n",
    "if (len(a) == n):\n",
    "    for i in range(0,n-1):\n",
    "        y = int(a[i])\n",
    "\n",
    "\n",
    "        if (x >= y):\n",
    "            print(y,end=' ')\n",
    "else:\n",
    "    print('n의 개수만큼 인덱스를 입력하세요.')\n",
    "    \n"
