   "source": [
    "x = int(input('양의 정수를 입력하세요.: '))\n",
    "\n",
    "\n",
    "for i in range(1,x+1):\n",
    "    print(' '*(x-i)+'*'*(2*i-x)+' '*(x-i))\n",
    "    \n",
    "for j in range(1,x+1):\n",
    "     print(' '*(j)+'*'*(x-2*j)+' '*(j))\n",
    "    "
